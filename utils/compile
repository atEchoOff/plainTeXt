mkdir -p dist
python3 utils/load_into.py src/main.js > dist/output.js
babel dist/output.js --out-file dist/output.js
./node_modules/.bin/esbuild dist/output.js --bundle --outfile=dist/output.js --allow-overwrite
cp mqeditor/build/mathquill.js dist/mathquill.js
cp mqeditor/build/mathquill.css style/mathquill.css
cp -r ./node_modules/@pyscript/core/dist dist/pyscript/
cp mqeditor/build/fonts/Symbola.ttf style/fonts
cp mqeditor/build/fonts/Symbola.woff style/fonts
cp mqeditor/build/fonts/Symbola.woff2 style/fonts